<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Button Style Primary -->
    <style name="ButtonPrimary">
        <item name="android:background">
            @drawable/button_primary_background
        </item>
        <item name="android:textColor">
            @android:color/white
        </item>
    </style>

    <!-- Button Style Success -->
    <style name="ButtonSuccess">
        <item name="android:background">
            @drawable/button_success_background
        </item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <!-- Button Style Warning -->
    <style name="ButtonWarning">
        <item name="android:background">
            @drawable/button_warning_background
        </item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <string name="ini_button_success">Ini Button Success</string>
    <string name="ini_button_warning">Ini Button Warning</string>
</resources>